<p id="notice"><%= notice %></p>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) if can? :manage, project %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to I18n.t("new_orga"), new_project_path, class: "btn btn-u pull-right" %>
